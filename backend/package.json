{
  "name": "nilo-project",
  "version": "0.0.0",
  "description": "Nilay Project",
  "scripts": {
    "lint": "lint-staged",
    "prepare": "husky install",
    "start": "node bin/www",
    "test": "jest --testEnvironment=node --verbose --forceExit --maxWorkers=1",
    "test-watch": "jest --testEnvironment=node --verbose --forceExit --watchAll --maxWorkers=1",
    "test-coverage": "jest --collect-coverage --testEnvironment=node --forceExit --maxWorkers=1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "./**/*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "keywords": [
    "software",
    "engineering",
    "nodejs",
    "backend"
  ],
  "author": "Nilay Aydin",
  "license": "MIT",
  "devDependencies": {
    "connect-livereload": "^0.6.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.5",
    "livereload": "^0.9.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "eslint-plugin-jest": "^26.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^6.1.7",
    "mongoose-autopopulate": "^0.16.0",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  }
}
